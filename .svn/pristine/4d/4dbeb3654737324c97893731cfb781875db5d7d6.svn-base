.tabsF {
  position: fixed !important;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 999;
}
.header .nav {
  height: 60.76388889px;
  background: #14131c;
  line-height: 60.76388889px;
  padding: 0 20.83333333px;
}
.header .nav .logo {
  width: 130.90277778px;
  vertical-align: middle;
}
.header .nav .menu {
  float: right;
  /*margin-top: 61/@scale;*/
  width: 69.44444444px;
  height: 100%;
  background: url("../images/menu.png") no-repeat;
  background-size: 30%;
  background-position: right center;
}
.header .menu-nav {
  padding-bottom: 52.08333333px;
  position: absolute;
  left: 0;
  width: 100%;
  z-index: 999;
  background: #FFF;
  display: none;
}
.header .menu-nav .menu-header {
  line-height: 60.76388889px;
  padding: 0 20.83333333px;
}
.header .menu-nav .menu-header img {
  width: 130.90277778px;
  vertical-align: middle;
}
.header .menu-nav .menu-header .close {
  width: 24.30555556px;
  height: 24.30555556px;
  float: right;
  margin-top: 19.09722222px;
  position: relative;
}
.header .menu-nav .menu-header .close .line {
  width: 100%;
  height: 3px;
  border-radius: 3px;
  background: #000;
  position: absolute;
  top: 0;
  bottom: 0;
  margin: auto;
}
.header .menu-nav .menu-header .close .line:nth-child(1) {
  -webkit-transform: rotate(45deg);
  transform: rotate(45deg);
}
.header .menu-nav .menu-header .close .line:nth-child(2) {
  -webkit-transform: rotate(-45deg);
  transform: rotate(-45deg);
}
.header .menu-nav .menu-body .menu-nav-list > li {
  /*padding: 0 60/@scale;*/
}
.header .menu-nav .menu-body .menu-nav-list > li .title {
  line-height: 41.66666667px;
  display: block;
  height: 100%;
  border-bottom: 1px solid #292929;
  margin-left: 20.83333333px;
  color: #454545;
  padding-right: 26.04166667px;
}
.header .menu-nav .menu-body .menu-nav-list > li .title i {
  display: inline-block;
  width: 12.15277778px;
  height: 12.15277778px;
  border-left: 1px solid #2c2c2c;
  border-bottom: 1px solid #2c2c2c;
  transform: rotate(-45deg);
  float: right;
  margin-top: 10.41666667px;
  transform-origin: 50% 50%;
}
.header .menu-nav .menu-body .menu-nav-list > li .title-active {
  border-bottom: 1px solid #45bcc7;
  color: #45bcc7;
}
.header .menu-nav .menu-body .menu-nav-list > li .title-active i {
  border-left: 1px solid #45bcc7;
  border-bottom: 1px solid #45bcc7;
  transform: rotate(135deg);
  transform-origin: 32% 80%;
}
.header .menu-nav .menu-body .menu-nav-list > li .menu-nav-list-item {
  background: #f0f0f0;
  overflow: hidden;
  display: none;
}
.header .menu-nav .menu-body .menu-nav-list > li .menu-nav-list-item > li {
  line-height: 41.66666667px;
  color: #cacaca;
  border-bottom: 1px solid #cacaca;
  margin-left: 20.83333333px;
}
.header .banner {
  height: 370.83333333px;
  position: relative;
  overflow: hidden;
}
.header .banner img {
  width: 100%;
  height: 100%;
  display: block;
}
.main .tabs {
